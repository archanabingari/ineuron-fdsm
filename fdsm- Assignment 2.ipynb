   "source": [
    "3.Logical AND truth table:\n",
    "  x    |y    |output\n",
    "  False|False|False\n",
    "  False|True |False\n",
    "  True |False|False\n",
    "  True |True |True\n",
    "  Logical OR truth table:\n",
    "  x    |y    |output\n",
    "  False|false|false\n",
    "  False|True|True\n",
    "  True|False|True\n",
    "  True|True|True\n",
    "  Logical NOT truth table:\n",
    "  x    |output\n",
    "  False|True\n",
    "  True|False"
   ]
